{% extends 'base.html.twig' %}

{% block title %}- Medias{% endblock %}

{% block body %}
<div class="container">
    <h1>Alice Medias!</h1>
    {% for media in medias %}
    {% if media.isVisible %}
        <iframe width="560" height="315" src="{{media.embedVideo}}" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
    {% endif %}
    {% endfor %}
</div>
{% endblock %}